extends ../template.jade

block vars
  - var title = "Registration | TJMUNQRO 2016"
  - var description = "Know what is our mission as a team planning the TJMUNQRO 2016, what you can expect from us, and why you should choose to assist"
  - var page = 'registration'
  
block cHead
  link(rel="stylesheet", href="style.css")
  
block content
  h1#pageTitle Registration
  #register-form
    .form-head(v-bind:class="{'school-form-head': schoolHead, 'independent-form-head': independentHead}")
      h4 Are you an independent or a school?
      #school-btn.button-option(v-on:click="openSchoolForm()") School
      #independent-btn.button-option(v-on:click="openIndepententForm()") Independent
    #google-form-container
      #independent-register-form(v-show="isIndependentForm")
        //- Place Here Independent forms
        iframe(id="gform", src="https://docs.google.com/forms/d/e/1FAIpQLSdRbU3jwj_NV3LE6kh7_IuUUgAGX1Mi_rFBOuuRdRH8Jfqw8Q/viewform?embedded=true", style="width:100%", frameborder="0", marginheight="0", marginwidth="0") Loading...
      #school-register-form(v-show="isSchoolForm")
        iframe(id="gform", src="https://docs.google.com/forms/d/e/1FAIpQLSdRbU3jwj_NV3LE6kh7_IuUUgAGX1Mi_rFBOuuRdRH8Jfqw8Q/viewform?embedded=true", style="width:100%", frameborder="0", marginheight="0", marginwidth="0") Loading...
        //- Place Here School forms
      
      .big-button(v-on:click="nextStep")
